    "karma-jasmine-html-reporter": "^1.0.0",
    "moment": "^2.24.0",
    "ng-packagr": "5.7.1",
    "protractor": "^5.4.2",
    "rimraf": "^3.0.0",
    "ts-node": "~8.5.4",
